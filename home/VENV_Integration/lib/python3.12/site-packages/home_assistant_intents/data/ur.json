        "one": "{{ slots.name | capitalize }} {{ state.state_with_unit }} ہے\n",
        "one_yesno": "{% if query.matched %}\n  جی ہاں\n{% else %}\n  نہیں، {{ state.state_with_unit }} ہے\n{% endif %}\n",
        "any": "{% if query.matched %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    جی، {{ match[:3] | join(\", \") }} اور {{ (match | length - 3) }} اور دوسری چیزیں بھی\n  {%- else -%}\n    جی،\n    {% for name in match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} اور {% endif -%}\n      {{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% else %}\n  نہیں\n{% endif %}\n",
        "all": "{% if not query.unmatched: %}\n  جی ہاں\n{% else %}\n  {% set no_match = query.unmatched | map(attribute=\"name\") | sort | list %}\n  {% if no_match | length > 4 %}\n    نہیں، {{ no_match[:3] | join(\", \") }} اور {{ (no_match | length - 3) }} اور نہیں ھیں\n  {%- else -%}\n    نہیں،\n    {% for name in no_match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} اور {% endif -%}\n      {{ name }}\n    {%- endfor %} نہیں ھیں\n  {% endif %}\n{% endif %}\n",
        "which": "{% if not query.matched %}\n  کوئی بھی نہیں\n{% else: %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    {{ match[:3] | join(\", \") }} اور {{ (match | length - 3) }} اور\n  {% else %}\n    {%- for name in match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} اور {% endif -%}\n      {{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% endif %}\n",
        "how_many": "{{ query.matched | length }}\n"
