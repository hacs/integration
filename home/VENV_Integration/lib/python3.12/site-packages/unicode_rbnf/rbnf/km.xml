<ldml>
    <identity>
        <version number="$Revision$"/>
        <language type="km"/>
    </identity>
    <rbnf>
        <rulesetGrouping type="SpelloutRules">
            <ruleset type="lenient-parse" access="private">
                <rbnfrule value="0">&amp;[last primary ignorable ] ←← ' ' ←← '​' ←← '­';</rbnfrule>
            </ruleset>
            <ruleset type="spellout-numbering-year">
                <rbnfrule value="0">=%spellout-numbering=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-numbering">
                <rbnfrule value="-x">ដក​→→;</rbnfrule>
                <rbnfrule value="x.x">←←​ចុច​→→→;</rbnfrule>
                <rbnfrule value="0">សូន្យ;</rbnfrule>
                <rbnfrule value="1">មួយ;</rbnfrule>
                <rbnfrule value="2">ពីរ;</rbnfrule>
                <rbnfrule value="3">បី;</rbnfrule>
                <rbnfrule value="4">បួន;</rbnfrule>
                <rbnfrule value="5">ប្រាំ;</rbnfrule>
                <rbnfrule value="6">ប្រាំមួយ;</rbnfrule>
                <rbnfrule value="7">ប្រាំពីរ;</rbnfrule>
                <rbnfrule value="8">ប្រាំបី;</rbnfrule>
                <rbnfrule value="9">ប្រាំបួន;</rbnfrule>
                <rbnfrule value="10">ដប់[​→→];</rbnfrule>
                <rbnfrule value="20">ម្ភៃ[​→→];</rbnfrule>
                <rbnfrule value="30">សាមសិប[​→→];</rbnfrule>
                <rbnfrule value="40">សែសិប[​→→];</rbnfrule>
                <rbnfrule value="50">ហាសិប[​→→];</rbnfrule>
                <rbnfrule value="60">ហុកសិប[​→→];</rbnfrule>
                <rbnfrule value="70">ចិតសិប[​→→];</rbnfrule>
                <rbnfrule value="80">ប៉ែតសិប[​→→];</rbnfrule>
                <rbnfrule value="90">កៅសិប[​→→];</rbnfrule>
                <rbnfrule value="100">←←​រយ[​→→];</rbnfrule>
                <rbnfrule value="1000">←←​ពាន់[​→→];</rbnfrule>
                <rbnfrule value="10000">←←​ម៉ឺន[​→→];</rbnfrule>
                <rbnfrule value="100000">←←​សែន[​→→];</rbnfrule>
                <rbnfrule value="1000000">←←​លាន[​→→];</rbnfrule>
                <rbnfrule value="10000000000">←←​ពាន់​កោដិ[​→→];</rbnfrule>
                <rbnfrule value="1000000000000">=#,##0=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-cardinal">
                <rbnfrule value="0">=%spellout-numbering=;</rbnfrule>
            </ruleset>
            <ruleset type="spellout-ordinal">
                <rbnfrule value="0">ទី=%spellout-cardinal=;</rbnfrule>
            </ruleset>
        </rulesetGrouping>
        <rulesetGrouping type="OrdinalRules">
            <ruleset type="digits-ordinal">
                <rbnfrule value="-x">−→→;</rbnfrule>
                <rbnfrule value="0">ទី=#,##0=;</rbnfrule>
            </ruleset>
        </rulesetGrouping>
    </rbnf>
